{
  "mongo": {
    "url": "mongodb://localhost/openhim-development",
    "atnaUrl": "mongodb://localhost/openhim-development"
  },
  "logger": {
    "level": "debug"
  },
  "statsd": {
    "host": "127.0.0.1"
  },
  "application": {
    "name": "openhim.development"
  },
  "auditing": {
    "servers": {
      "udp": {
        "enabled": true,
        "port": 5050
      },
      "tls": {
        "enabled": true,
        "port": 5051
      },
      "tcp": {
        "enabled": true,
        "port": 5052
      }
    }
  },
  "certificateManagement": {
    "watchFSForCert": false,
    "certPath": "resources/certs/default/cert.pem",
    "keyPath": "resources/certs/default/key.pem"
  }
}
